#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int T;
    cin>>T;
    string str[T];
    for(int i=0;i<T;i++)
    cin>>str[i];
    for(int j=0;j<T;j++)
    {
        string a = str[j];
        for(int k=0;k<a.length();k++)
        {
            if(k%2==0)
            cout<<a[k];
        }
        cout<<" ";
        for(int l=0;l<a.length();l++)
        {
            if(l%2!=0)
            cout<<a[l];
        }
        cout<<endl;
    }
    
    
    return 0;
}
